<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smm.mapper.TestMapper">
    <resultMap id="testMap" type="com.smm.entity.Test">
        <id property="id" column="id"></id>
        <result property="test" column="test"></result>
    </resultMap>

    <select id="testFindAll" resultMap="testMap">
        select * from test
    </select>
</mapper>